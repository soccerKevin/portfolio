<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Technologies used</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="all_projects">
    <% @projects.each do |project| %>
      <%= render project %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path, remote: true, id: :add_project %>
